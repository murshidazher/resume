{
  "name": "@murshidazher/resume",
  "version": "1.0.0",
  "description": "A simple tool to automate my resume generation.",
  "main": "bin/www",
  "engines": {
    "node": "18.13.0",
    "npm": "9.2.0"
  },
  "scripts": {
    "start": "NODE_ENV=dev node ./bin/www",
    "start:prod": "NODE_ENV=prod node ./bin/www",
    "convert": "node ./bin/convert",
    "gen:pdf": "npm run convert -- src/examples/index.html out/index.pdf --data '{\"name\": \"Awesome\", \"dateformat\": \"MMM YYYY\", \"content\": \"Lorem ipsum dolor.\"}'"
  },
  "author": "Murshid Azher",
  "license": "MIT",
  "dependencies": {
    "bluebird": "3.7.2",
    "body-parser": "1.20.1",
    "chalk": "5.2.0",
    "cli-spinner": "0.2.10",
    "commander": "10.0.0",
    "cookie-parser": "1.4.6",
    "dayjs": "^1.9.4",
    "debug": "4.3.4",
    "express": "4.18.2",
    "hjs": "0.0.6",
    "html-pdf": "~3.0.1",
    "morgan": "~1.10.0",
    "serve-favicon": "2.5.0",
    "upath": "2.0.1"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/murshidazher/resume.git"
  },
  "keywords": [
    "Resume",
    "PDF",
    "HTML",
    "CV"
  ],
  "bugs": {
    "url": "https://github.com/murshidazher/resume/issues"
  },
  "homepage": "https://github.com/murshidazher/resume#readme"
}
